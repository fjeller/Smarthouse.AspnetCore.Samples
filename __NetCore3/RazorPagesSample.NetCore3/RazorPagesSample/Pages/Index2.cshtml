@page
@using RazorPagesSample.DataContext
@model RazorPagesSample.Pages.Index2Model
@{
	ViewData["Title"] = "Index2";
}

<partial name="PersonList" model="Model.People" />

<form asp-page="Index" method="post">
	<div class="form-group">
		<label asp-for="FirstName" class="col-form-label"></label>
		<input asp-for="FirstName" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="LastName" class="col-form-label"></label>
		<input asp-for="LastName" class="form-control" />
	</div>

	<div>
		<button type="submit">Submit</button>
	</div>
</form>

